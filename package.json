{
  "name": "eatitin-delivery",
  "version": "1.0.0",
  "private": true,
  "homepage": "http://delivery.eatitin.com",
  "main": "",
  "dependencies": {
    "@feathersjs/client": "^4.3.0",
    "@feathersjs/rest-client": "^4.3.0",
    "@rematch/core": "^1.1.0",
    "antd": "^3.23.1",
    "axios": "^0.19.0",
    "chart.js": "^2.8.0",
    "firebase": "^6.5.0",
    "idb-keyval": "^3.2.0",
    "inspector": "^0.5.0",
    "js-cookie": "^2.2.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "moment": "^2.24.0",
    "qs": "^6.8.0",
    "react": "^16.9.0",
    "react-chartjs-2": "^2.8.0",
    "react-detect-offline": "^2.4.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-infinite-scroll-component": "^4.5.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^4.0.8",
    "react-scroll-up": "^1.3.3",
    "reactjs-localstorage": "^0.0.7",
    "timeago.js": "^4.0.0-beta.2"
  },
  "scripts": {
    "start": "PORT=7000 REACT_APP_ENV=development react-app-rewired start",
    "sass:watch": "node-sass -w src/assets/scss/style.scss -o src/assets/css",
    "sass:build": "node-sass src/assets/scss/style.scss -o src/assets/css --source-map true",
    "design": "PORT=7000 react-app-rewired start",
    "dev": "npm-run-all --parallel design sass:watch",
    "start:stag": "react-app-rewired start",
    "test": "react-app-rewired test --env=jsdom",
    "lint": "eslint src/app/. --config .eslintrc.json",
    "build-sw": "node ./src/sw-build.js",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build": "GENERATE_SOURCEMAP=false REACT_APP_ENV=development react-app-rewired build && yarn run build-sw && yarn run clean-cra-sw",
    "stag": "GENERATE_SOURCEMAP=false REACT_APP_ENV=staging react-app-rewired build && yarn run build-sw && yarn run clean-cra-sw",
    "prod": "GENERATE_SOURCEMAP=false REACT_APP_ENV=production react-app-rewired build && yarn run build-sw",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-app-rewired eject"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-import": "^1.12.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "customize-cra": "^0.5.0",
    "express": "^4.17.1",
    "flow-bin": "^0.106.3",
    "metro-react-native-babel-preset": "^0.56.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^3.1.1",
    "react-test-renderer": "^16.9.0",
    "source-map-explorer": "^2.0.1",
    "workbox-build": "^4.3.1"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "jest": {},
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
